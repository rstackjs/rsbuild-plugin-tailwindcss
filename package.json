{
  "name": "rsbuild-plugin-tailwindcss",
  "version": "0.2.3",
  "repository": "https://github.com/rstackjs/rsbuild-plugin-tailwindcss",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "bump": "npx bumpp",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "playwright test"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.54.1",
    "@rollup/pluginutils": "^5.2.0",
    "@rsbuild/core": "^1.4.12",
    "@rslib/core": "^0.11.0",
    "@rsbuild/webpack": "^1.3.2",
    "@types/node": "^22.17.0",
    "@types/semver": "^7.7.0",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "semver": "^7.7.2",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.1.0",
    "tailwindcss": "^3.1.0"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
