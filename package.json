{
  "name": "rsbuild-plugin-tailwindcss",
  "version": "0.2.3",
  "repository": "https://github.com/rstackjs/rsbuild-plugin-tailwindcss",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "rslib build",
    "bump": "npx bumpp",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "playwright test --workers 1"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@playwright/test": "^1.58.2",
    "@rollup/pluginutils": "^5.3.0",
    "@rsbuild/core": "2.0.0-beta.4",
    "@rslib/core": "^0.19.5",
    "@types/node": "^22.19.11",
    "@types/semver": "^7.7.1",
    "postcss": "^8.5.6",
    "semver": "^7.7.4",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.1.0 || ^2.0.0-0",
    "tailwindcss": "^3.1.0"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.29.3",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
